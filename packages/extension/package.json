{
  "name": "code-fitness-extension",
  "version": "0.0.0",
  "description": "",
  "author": "Inesh Bose <2504266B@student.gla.ac.uk>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/ineshbose/code-fitness"
  },
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:code-fitness.start",
    "onCommand:code-fitness.reload"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-fitness.start",
        "title": "Start new coding session"
      },
      {
        "command": "code-fitness.reload",
        "title": "Reload coding session"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsup src/index.ts --external vscode",
    "dev": "npm run build -- --watch",
    "pack": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.69.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "@vscode/vsce": "^2.15.0"
  }
}
