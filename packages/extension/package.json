{
  "name": "extension",
  "version": "0.0.0",
  "description": "",
  "author": "Inesh Bose <2504266B@student.gla.ac.uk>",
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "icon": "https://raw.githubusercontent.com/ineshbose/code-fitness/main/.github/logo.png",
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:code-fitness.start",
    "onCommand:code-fitness.reload"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-fitness.start",
        "title": "Open dashboard",
        "category": "Code Fitness",
        "icon": "$(code-fitness)"
      }
    ],
    "icons": {
      "code-fitness": {
        "description": "Code Fitness icon",
        "default": {
          "fontPath": "dist/cf-font.woff",
          "fontCharacter": "\\EA01"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "clean": "rimraf .turbo node_modules dist",
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "pack": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "app": "*",
    "ofetch": "^1.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@vscode/vsce": "^2.15.0",
    "tsup": "^6.5.0"
  },
  "build": {
    "externals": [
      "vscode"
    ]
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "external": [
      "vscode"
    ],
    "noExternal": [
      "app"
    ],
    "publicDir": "../app/dist"
  }
}
