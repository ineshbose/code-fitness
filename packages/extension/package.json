{
  "name": "extension",
  "version": "0.0.0",
  "description": "",
  "author": "Inesh Bose <2504266B@student.gla.ac.uk>",
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:code-fitness.start",
    "onCommand:code-fitness.reload"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-fitness.start",
        "title": "Start new coding session"
      },
      {
        "command": "code-fitness.reload",
        "title": "Reload coding session"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "clean": "rimraf .turbo node_modules dist",
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "pack": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "app": "*"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@vscode/vsce": "^2.15.0",
    "tsup": "^6.5.0"
  },
  "build": {
    "externals": [
      "vscode"
    ]
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "external": [
      "vscode"
    ],
    "noExternal": [
      "app"
    ],
    "publicDir": "../app/dist"
  }
}
